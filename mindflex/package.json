{
    "name": "mindflex_nodecopter",
    "description": "takes 3 serial port inputs from hacked mindflex headsets. the inputs are linked with nodecopter commands and you can fly the drone. also uses dronestream for a video feed.",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "git@github.com:talamantez/mindflex-nodecopter.git"
    },
    "main": "index",
    "keywords": [
        "drone",
        "nodecopter",
        "parrot",
        "video",
        "stream",
        "browser",
        "x264",
        "mindflex",
        "eegflight"
    ],
    "dependencies": {
        "ws": "~0.4.27",
        "ar-drone": "0.1.0",
        "buffy": "0.0.5",
        "bindings": "1.1.0",
        "async": "0.1.18",
        "sf": "0.1.6",
        "optimist": "~0.3.4",
        "serialport": "1.1.3",
        "dronestream": "1.1.0",
        "node-gyp": "*"
    },
    "devDependencies": {
        "grunt": "~0.4.1",
        "grunt-contrib-uglify": "~0.2.2",
        "grunt-contrib-jshint": "~0.6.2",
        "matchdep": "~0.1.2",
        "grunt-contrib-concat": "~0.3.0",
        "mocha": "*",
        "chai": "*"
    },
    "engines": {
        "node": ">= 0.7.0"
    },
    "bin": {
        "serialportlist": "./bin/serialportList.js",
        "serialportterm": "./bin/serialportTerminal.js"
    },
    "scripts": {
        "test": "mocha -R spec -t 60s -s 60s",
        "install": "node-gyp rebuild"
    },

    "contributors": [{
        "name": "Robert Talamantez",
        "email": "robert.talamantez2@gmail.com"
    }],
    "license": "BSD",
    "readmeFilename": "README.md",
    "_id": "mindflex_nodecopter@0.0.1"
}
